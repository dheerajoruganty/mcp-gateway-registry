# AgentCore Gateway Access Token Configuration
# Copy this file to config.yaml and update with your actual values

# Gateway Configuration
gateway_arn: "arn:aws:bedrock-agentcore:us-east-1:YOUR_ACCOUNT_ID:gateway/YOUR_GATEWAY_NAME"
server_name: "your-server-name"

# Cognito Configuration (for token generation)
user_pool_id: "us-west-2_EXAMPLE123"
client_id: "your_client_id_here"

# Note: Client secret should be provided via environment variable COGNITO_CLIENT_SECRET
# Do not store secrets in this file!